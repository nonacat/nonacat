{
  "name": "nonacat",
  "version": "1.0.0",
  "description": "nonacat a lazy bot, does the things on behalf of you.",
  "main": "app.js",
  "scripts": {
    "start": "node dist/app.js",
    "dev": "./node_modules/.bin/nodemon app.ts",
    "dist": "./node_modules/.bin/tsc",
    "lint-check": "./node_modules/.bin/tslint --project .",
    "lint-fix": "./node_modules/.bin/tslint --project . --fix",
    "clean": "rm -rf ./dist",
    "docker-build": "docker build -t nonacat/nonacat:ci ."
  },
  "author": "nonacat dev",
  "license": "ISC",
  "dependencies": {
    "@octokit/rest": "17.5.1",
    "body-parser": "1.19.0",
    "express": "4.17.1",
    "express-pino-logger": "4.0.0",
    "js-yaml": "3.13.1",
    "pino": "6.2.0",
    "pino-pretty": "4.0.0",
    "tslint": "6.1.1"
  },
  "devDependencies": {
    "@types/express": "4.17.6",
    "@types/express-pino-logger": "4.0.2",
    "@types/js-yaml": "3.12.3",
    "@types/node": "13.13.1",
    "@types/pino": "6.0.0",
    "nodemon": "2.0.3",
    "ts-node": "8.9.0",
    "typescript": "3.8.3"
  }
}
